{% extends "RestooMainBundle::layout.html.twig" %}

{% block title "Jobs" %}
{% block subheadline "Jobs" %}

{% block submenu %}
	{% include 'RestooMainBundle:Job:submenu.html.twig' %}
{% endblock %}

{% block content %}

<h1>Job edit</h1>
<form action="{{ path('job_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('job') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}